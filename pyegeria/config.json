
{
        "log_directory": "./logs",
        "console_logging_level": "INFO",
        "logging_console_format": "<green>{time:YYYY-MM-DD HH:mm:ss}</green> | <level>{level}</level> | <cyan>{module}</cyan>:<cyan>{line}</cyan> - <level>{message}</level>",

        "file_logging_level": "TRACE",
        "logging_file_format": "{time:YYYY-MM-DD HH:mm:ss} | {level} | {module}:{line} - {message}",
        "timeout_seconds": 30,
        "debug_mode": false,
        "feature_x_enabled": false
}